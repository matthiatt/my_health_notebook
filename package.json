  "dependencies": {
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "googleapis": "^66.0.0",
    "if-env": "^1.0.4",
    "mongoose": "^5.11.5",
    "nodemailer": "^6.4.16"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "nodemon": "^1.18.7"
  }
